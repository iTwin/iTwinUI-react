{
  "name": "website",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview"
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "^0.19.6",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@astrojs/react": "^0.1.1",
    "@itwin/itwinui-react": "*",
    "@types/react": "^17.0.0",
    "astro": "^1.0.0-beta.27",
    "postcss-preset-env": "^7.4.4",
    "prettier": "2.6.2",
    "prettier-plugin-astro": "^0.1.0-next.4",
    "react-docgen-typescript": "^2.2.2"
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": {
        "features": {
          "nesting-rules": "true"
        }
      }
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "jsxSingleQuote": true,
    "singleQuote": true,
    "tabWidth": 2,
    "plugins": [
      "../../node_modules/prettier-plugin-astro"
    ],
    "astroAllowShorthand": false,
    "overrides": [
      {
        "files": "**/*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  }
}
